{
    "name": "Unbabel Balance",
    "version": "1.0",
    "description": "Shows the current balance for Unbabel",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    
    "browser_action":{
        "default_popup": "popup.html"
    },
    "permissions": [
        "*://*.unbabel.com/*",
        "storage",
        "tabs"
    ],
    "icons":{
        "32": "icon.png"
    }
}
